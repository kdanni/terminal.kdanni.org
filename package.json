{
  "name": "terminal-kdanni-org",
  "version": "0.1.0",
  "private": true,
  "description": "Personal Bloomberg-style terminal for financial time series aggregation and analysis.",
  "workspaces": [
    "collector"
  ],
  "scripts": {
    "start:collector": "npm --workspace collector run start",
    "test": "npm --workspace collector run test",
    "db:migrate": "docker compose run --rm flyway migrate",
    "db:info": "docker compose run --rm flyway info"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9"
  }
}
